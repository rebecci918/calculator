        % Button pushed function: Button_13
        function Button_13Pushed(app, event)
            app.mem1 = 'X';
            if app.EditField.Value == '#'
                app.EditField.Value = '#';
            else
                app.EditField.Value = strcat(app.EditField.Value,'+');
            end
        end

        % Button pushed function: Button_14
        function Button_14Pushed(app, event)
            app.mem1 = 'X';
            if app.EditField.Value == '#'
                app.EditField.Value = '#';
            else
                app.EditField.Value = strcat(app.EditField.Value,'-');
            end
        end

        % Button pushed function: xButton
        function xButtonPushed(app, event)
            app.mem1 = 'X';
            if app.EditField.Value == '#'
                app.EditField.Value = '#';
            else
                app.EditField.Value = strcat(app.EditField.Value,'*');
            end
        end

        % Button pushed function: Button_12
        function Button_12Pushed(app, event)
            app.mem1 = 'X';
