        % Button pushed function: Button_16
        function Button_16Pushed(app, event)
            if app.test == 1 
                app.mem1 = 'X';
                app.EditField.Value = '(';
            else
                 if app.EditField.Value == '#'
                     app.EditField.Value = '(';
                 else
                 app.EditField.Value = strcat(app.EditField.Value,'(');
                 end
            end
        end

        % Button pushed function: Button_15
        function Button_15Pushed(app, event)
            app.mem1 = 'X';
            if app.EditField.Value == '#'
                app.EditField.Value = '#';
            else
                app.EditField.Value = strcat(app.EditField.Value,'^');
            end
        end

        % Button pushed function: CLEARButton
        function CLEARButtonPushed(app, event)
            default = '#';
            app.EditField.Value = default;
        end

        % Button pushed function: ENTERButton
        function ENTERButtonPushed(app, event)
            x = eval(app.EditField.Value);
            y = num2str(x);
            app.mem1 = y;
            app.EditField.Value = app.mem1;
        end

        % Button pushed function: MButton
        function MButtonPushed(app, event)
            app.mem2 = app.EditField.Value;
        end

        % Button pushed function: CLCMEMButton
        function CLCMEMButtonPushed(app, event)
            app.mem2 = '#';
        end

        % Button pushed function: RMButton
        function RMButtonPushed(app, event)
            if app.EditField.Value == '#'
                app.EditField.Value = app.mem2;
            elseif app.mem2 == '#'
                
            else
                app.EditField.Value = strcat(app.EditField.Value,app.mem2);
            end
        end

        % Callback function
        function MButton_2Pushed(app, event)
            
        end
    end

    % Component initialization
    methods (Access = private)

        % Create UIFigure and components
        function createComponents(app)

            % Create UIFigure and hide until all components are created
            app.UIFigure = uifigure('Visible', 'off');
            app.UIFigure.Color = [0 0 0];
            app.UIFigure.Position = [100 100 420 522];
            app.UIFigure.Name = 'MATLAB App';

            % Create Button
            app.Button = uibutton(app.UIFigure, 'push');
            app.Button.ButtonPushedFcn = createCallbackFcn(app, @ButtonPushed, true);
            app.Button.Tag = 'btn_1';
            app.Button.BackgroundColor = [0 1 0];
            app.Button.FontSize = 20;
            app.Button.Position = [21 103 60 60];
            app.Button.Text = '1';

            % Create Button_2
            app.Button_2 = uibutton(app.UIFigure, 'push');
