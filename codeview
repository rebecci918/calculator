            app.Button_3.Tag = 'btn_3';
            app.Button_3.BackgroundColor = [0 1 0];
            app.Button_3.FontSize = 20;
            app.Button_3.Position = [181 103 60 60];
            app.Button_3.Text = {'3'; ''};

            % Create Button_4
            app.Button_4 = uibutton(app.UIFigure, 'push');
